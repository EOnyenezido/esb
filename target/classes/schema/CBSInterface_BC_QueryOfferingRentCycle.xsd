						<xs:element name="ProcessedCycleNum" type="xs:integer" minOccurs="0"/>
						<xs:element name="SuccessCycleNum" type="xs:integer" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
